// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Character {
  id          Int    @id @default(autoincrement())
  photoUrl    String
  name        String
  gender      String
  eye         String
  hair        String
  skin        String
  love        String
  weapon      String
  profession  String
  position    String
  predecessor String
  affiliation String
  first       String
  voicedBy    String
}
